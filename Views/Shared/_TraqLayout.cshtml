    <style>
        table.dataTable.dtr-inline.collapsed > tbody > tr[role="row"] > td.dtr-control, table.dataTable.dtr-inline.collapsed > tbody > tr[role="row"] > th.dtr-control {
            position: relative;
            padding-left: 30px !important;
            cursor: pointer;
            width: 54px !important;
        }

        .panel-warning > .panel-heading + .panel-collapse > .panel-body {
            border-top-color: ##9929ea !important;
        }


        .BoToDoHide {
            display: none;
        }

        .button1 {
            background-color: #3f51b5 !important;
            border: none;
            color: white;
            padding: 15px 32px;
            text-align: center;
            text-decoration: none;
            display: inline-block;
            font-size: 16px;
            margin: 4px 2px;
            cursor: pointer;
        }

        .sk-cube-grid {
            width: 60px;
            height: 60px;
            text-align: center;
            font-size: 10px;
            background-color: grey;
        }

        .panel-warning {
            border-color: ##9929ea;
        }

        .sk-cube-grid .sk-cube-child {
            width: 33%;
            height: 33%;
            background-color: white;
            float: left;
            -webkit-animation: sk-cubeGridScaleDelay 1.3s infinite ease-in-out;
            animation: sk-cubeGridScaleDelay 1.3s infinite ease-in-out
        }

        .sk-cube-grid .sk-cube-grid9 {
            -webkit-animation-delay: .2s;
            animation-delay: .2s
        }


        .modal-dialog {
            position: relative;
            display: table; /* This is important */
            overflow-y: auto;
            overflow-x: auto;
            width: auto;
            min-width: 300px;
        }

        .btn {
            display: inline-block;
            margin-bottom: 0;
            font-weight: normal;
            text-align: center;
            vertical-align: middle;
            touch-action: manipulation;
            cursor: pointer;
            background-image: none;
            border: 1px solid #0089ff;
            white-space: nowrap;
            padding: 6px 15px;
            font-size: 14px;
            line-height: 1.57142857;
            border-radius: 2px;
            -webkit-user-select: none;
            -moz-user-select: none;
            -ms-user-select: none;
            user-select: none;
        }


        table.dataTable {
            clear: both !important;
            margin-top: 6px !important;
            margin-bottom: 6px !important;
            max-width: none !important;
            border-collapse: separate !important;
            border-spacing: 0 !important;
            width: 100% !important;
        }

        .required-field::after {
            content: '*';
            color: red;
        }


        input[type='date'] {
            position: relative;
        }

        ::-webkit-datetime-edit, ::-webkit-clear-button, ::-webkit-calendar-picker-indicator {
            position: absolute;
        }

        ::-webkit-clear-button {
            right: 2.5em;
        }

        ::-webkit-inner-spin-button {
            -webkit-appearance: none;
            margin: 0;
        }

        ::-webkit-calendar-picker-indicator {
            right: 0;
            padding: .539em .34em;
        }

        input[type="date"]::-webkit-calendar-picker-indicator {
            color: rgba(0, 0, 0, 0);
            opacity: 1;
            display: block;
            background: url('../../Content/Images/calenderIcon.png') no-repeat;
            background-size: 20px 20px;
            width: 20px;
            height: 20px;
            border-width: thin;
            margin-left: 50px;
        }

        .table > thead > tr > th, .table > tbody > tr > td, .table > tfoot > tr > td {
            vertical-align: middle !important;
            padding-left: 10px !important;
            padding-right: 10px !important;
        }

        table.dataTable thead .sorting_asc:after {
            content: "\2193";
        }

        table.dataTable thead .sorting_desc:after {
            content: "\2193";
            opacity: 1;
        }

        table.dataTable thead .sorting:after {
            opacity: 0.2;
            content: "\2193";
        }

        .dataTables_length {
            display: none;
        }




        /*AJC New color-profile Added*/
        .site-head {
            background-color: ##9929ea;
        }

        .main-container .nav-wrap .nav-head {
            background: ##9929ea;
        }

        .button1 {
            background-color: ##9929ea !important;
        }

        .btns {
            color: ##9929ea !important;
            border-color: ##9929ea !important;
            background-color: white !important;
        }

            .btns:hover {
                color: white !important;
                border-color: white !important;
                background-color: ##9929ea !important;
            }

        .btnn {
            color: ##9929ea !important;
            border-color: ##9929ea !important;
            background-color: white !important;
        }

            .btnn:hover {
                color: white !important;
                border-color: white !important;
                background-color: ##9929ea !important;
            }

        .btn {
            color: ##9929ea !important;
            border-color: ##9929ea !important;
            background-color: white !important;
        }

            .btn:hover {
                color: white !important;
                border-color: white !important;
                background-color: ##9929ea !important;
            }

        .btn-default:hover {
            color: white;
            border-color: white;
            background-color: ##9929ea !important;
        }


        .pagination > .active > a, .pagination > .active > span, .pagination > .active > a:hover, .pagination > .active > span:hover, .pagination > .active > a:focus, .pagination > .active > span:focus {
            color: #ffffff !important;
            background-color: grey !important;
            border-color: #ffffff !important;
        }

        .headerrow > th {
            color: white !important;
        }

        .headerrow {
            background-color: ##9929ea !important;
        }

        .fa {
            color: ##9929ea !important;
        }

        .tab .nav-tabs li a:hover, .tab .nav-tabs li.active a {
            background: white !important;
            color: orange !important;
            border-color: orange !important;
            border: 1px !important;
        }

        .modal-open .modal {
            overflow-x: hidden;
            overflow-y: auto;
            background: #808080d1;
        }

        .div.nav-head {
            background-color: ##9929ea !important;
        }

        * {
            font-family: Arial !important;
        }

        .fa {
            font: normal normal normal 14px/1 FontAwesome !important;
        }

        .panel-warning > .panel-heading {
            color: #ffffff;
            background-color: ##9929ea;
            border-color: ##9929ea;
        }

        .main-container {
            width: 100%;
            overflow: hidden;
            position: relative;
            background: #ffffff !important;
        }

        body {
            letter-spacing: .5px;
            background: #ffffff;
        }


        /*AJC New color-profileAdded End*/
        .ui-icon {
            display: unset !important;
            vertical-align: middle;
            margin-top: -.25em;
            position: relative !important;
            text-indent: -99999px;
            overflow: hidden;
            background-repeat: no-repeat;
        }

        .ui-datepicker .ui-datepicker-prev span, .ui-datepicker .ui-datepicker-next span {
            display: block;
            position: absolute;
            left: 50% !important;
            margin-left: -16px !important;
