</head>
<body id="app" class="app off-canvas">
    <!-- header -->
    <header class="site-head" id="site-head">
        <ul class="list-unstyled left-elems">
            <li>
                <a href="javascript:;" class="nav-trigger ion ion-drag"></a>
            </li>

            <li>
                <div class="site-logo visible-xs">
                    <a href="javascript:;" class="text-uppercase h3">
                        <span class="text">TSA</span>
                    </a>
                </div>
            </li>
        </ul>
    </header>
    <!-- #end header -->
    <!-- main-container -->
    <div class="main-container clearfix">
        <!-- main-navigation -->
        <aside class="nav-wrap" id="site-nav" data-perfect-scrollbar>
            <div class="nav-head">
                <!-- site logo -->
                <a href="~/Home/Index" class="site-logo text-uppercase">
                    @*<i class="ion ion-monitor"></i>*@
                    <img src="~/Content/logo.png" style="width:36px; top:-2px; position:relative; right:6px;" />
                    <span class="text" align="center">TSA</span>
                </a>
            </div>

            <!-- Site nav (vertical) -->

            <nav class="site-nav clearfix" role="navigation">
                <!-- navigation -->
                <ul class="list-unstyled clearfix nav-list mb15">


                    @if (Request.IsAuthenticated)
                    {
                        using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm" }))
                        {
                            @Html.AntiForgeryToken()


                        }
                        <li>
                            <a  href="javascript:document.getElementById('logoutForm').submit()">
                                <i class="ion ion-power"></i>
                                <span class="text">Log Off</span>
                                <i class=""></i>
                            </a>
                        </li>
                    }
                    else
                    {

                        <li>
                            <a  href="~/Account/Login">
                                <i class="ion ion-log-in"></i>
                                <span class="text">Login</span>
                                <i class=""></i>
                            </a>
                        </li>

                    }
                    <li>
                        <a  href="~/Home/Index">
                            <i class="ion ion-log-in"></i>
                            <span class="text">Home</span>
                            <i class=""></i>
                        </a>
                    </li>
                    @if (Request.IsAuthenticated)
                    {
                        <li>
                            <a  href="~/People/Index">
                                <i class="ion ion-log-in"></i>
                                <span class="text">Persons</span>
                                <i class=""></i>
                            </a>
                        </li>
                    }
                    <li>
                        <a  href="~/Home/About">
                            <i class="ion ion-person"></i>
                            <span class="text">About</span>
                            <i class=""></i>
                        </a>
                    </li>
                    <li>
                        <a  href="~/Home/Contact">
                            <i class="ion ion-person"></i>
                            <span class="text">Contact</span>
                            <i class=""></i>
                        </a>
                    </li>
                    @using Microsoft.AspNet.Identity
                </ul> <!-- #end navigation -->
            </nav>


        </aside>
        <!-- #end main-navigation -->
        <!-- content-here -->
        <div class="content-container" id="content">
            <!-- dashboard page -->
            <div class="page page-dashboard">
                <div class="page-wrap">
                    @RenderBody()
                    <hr />

                </div> <!-- #end page-wrap -->
            </div>
            <!-- #end dashboard page -->
        </div>

    </div> <!-- #end main-container -->
    <!-- theme settings -->
    <div class="site-settings clearfix hidden-xs">
        <div class="settings clearfix">
            <div class="wrapper left" hidden>
                <ul class="list-unstyled other-settings">
                    <li class="clearfix mb10">
                        <div class="left small">Fixed Header</div>
                        <div class="md-switch right">
                            <label>
                                <input type="checkbox" id="fixedHeader">
                                <span>&nbsp;</span>
                            </label>
                        </div>
                    </li>
                    <li class="clearfix mb10">
                        <div class="left small">Nav Full</div>
                        <div class="md-switch right">
                            <label>
                                <input type="checkbox" id="navFull">
                                <span>&nbsp;</span>
                            </label>
                        </div>
                    </li>
                </ul>
                <hr />
                <ul class="themes list-unstyled" id="themeColor">
                    <li data-theme="theme-zero" class="active"></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="modal fade" id="confirmationModal" role="dialog" style="min-width: 0% !important;">
        <div class="modal-dialog" style="width: 35%;
        min-width: 0% !important;">
            <div class="modal-content">
                <div class="modal-header" style="background-color: ##9929ea;color:white;
        margin: 0px -1px !important;">
                    <h4 id="modalHeading" align="center" class="modal-title">Confirmation</h4>
                </div>
                <div class="modal-body">
                    <label id="confirmationMessage"></label>
                </div>
                <div class="modal-footer" style="align-content: flex-end">
                    <div class="form-group col-md-9">

                        <button id="btn" class="btn btn-primary" data-dismiss="modal" style="width:30% !important;">Cancel</button>
                        <button id="confirmationBtn" class="btn btn-primary" data-dismiss="modal" style="width:30% !important;">Confirm</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="genericMessageModal" role="dialog" style="min-width: 0% !important;">
        <div class="modal-dialog" style="width: 35%;
        min-width: 0% !important;">
            <div class="modal-content">
                <div class="modal-header" style="background-color: ##9929ea;color:white;
        margin: 0px -1px !important;">
                    <h4 id="modalHeading" align="center" class="modal-title">Attention</h4>
                </div>
                <div class="modal-body">
                    <label id="genericMessage"></label>
                </div>
                <div class="modal-footer" style="align-content: flex-end">
                    <div class="form-group col-md-7">

                        <button onclick="ZInd()" id="btn" class="btn btn-primary" data-dismiss="modal" style="width:30% !important;">Okay</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script>
                    //AJC GENERIC CONFIRMATION FOR ENTIRE APP
                    function genericConfirmation(buttonId, Message) {
                        document.getElementById("confirmationMessage").innerHTML = Message;
        $('#confirmationModal').modal({
                        backdrop: 'static',
            keyboard: false,
            show: true
        });
        $('#confirmationBtn').on('click', function () {
                        document.getElementById(buttonId).click();
                    })
    }

    function genericSystemMessage(Message) {
                        document.getElementById("genericMessage").innerHTML = Message;
        $('#genericMessageModal').modal({
                        backdrop: 'static',
            keyboard: false,
            show: true
        });
    }
    </script>
</body>
</html>