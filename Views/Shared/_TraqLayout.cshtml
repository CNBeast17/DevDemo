        /*Ash Style*/
        .TBCHead {
            width: 100%;
            background-image: linear-gradient(120deg,##9929ea 60%, grey 9%);
            color: white;
        }

        .TBCHead1 {
            background-color: white !important;
            color: ##9929ea !important;
        }

        .TBCHr {
